{
  "workbench.startupEditor": "none",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.largeFileOptimizations": false,
  "git.suggestSmartCommit": false,
  "workbench.iconTheme": "vscode-icons",
  "window.menuBarVisibility": "classic",
  "editor.tabCompletion": "on",
  "editor.formatOnType": true,
  "editor.selectionHighlight": true,
  "editor.wordBasedSuggestions": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.rulers": [120]
  },
  "dart.lineLength": 120,
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.debugSdkLibraries": false,
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#E57373",
    "activityBar.activeBorder": "#E57373",
    "list.activeSelectionForeground": "#E57373",
    "list.inactiveSelectionForeground": "#E57373",
    "list.highlightForeground": "#E57373",
    "scrollbarSlider.activeBackground": "#E5737350",
    "editorSuggestWidget.highlightForeground": "#E57373",
    "textLink.foreground": "#E57373",
    "progressBar.background": "#E57373",
    "pickerGroup.foreground": "#E57373",
    "tab.activeBorder": "#E57373",
    "notificationLink.foreground": "#E57373",
    "editorWidget.resizeBorder": "#E57373",
    "editorWidget.border": "#E57373",
    "settings.modifiedItemIndicator": "#E57373",
    "settings.headerForeground": "#E57373",
    "panelTitle.activeBorder": "#E57373",
    "breadcrumb.activeSelectionForeground": "#E57373",
    "menu.selectionForeground": "#E57373",
    "menubar.selectionForeground": "#E57373",
    "editor.findMatchBorder": "#E57373",
    "selection.background": "#E5737340",
    "statusBarItem.remoteBackground": "#E57373",
    "statusBar.background": "#8FBCBB",
    "statusBar.noFolderBackground": "#8FBCBB",
    "statusBar.debuggingBackground": "#8FBCBB",
    "statusBar.foreground": "#434C5E",
    "statusBar.debuggingForeground": "#434C5E"
  },
  "settingsSync.ignoredSettings": ["-dart.flutterSdkPaths"],
  "todohighlight.exclude": [
    "**/node_modules/**",
    "**/bower_components/**",
    "**/dist/**",
    "**/build/**",
    "**/.vscode/**",
    "**/.github/**",
    "**/_output/**",
    "**/*.min.*",
    "**/*.map",
    "**/.next/**"
  ],
  "todohighlight.include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.html",
    "**/*.php",
    "**/*.css",
    "**/*.scss"
  ],
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    "NOTE:",
    {
      "text": "HACK:",
      "color": "#000",
      "isWholeLine": false
    }
  ],
  "todohighlight.isEnable": false,
  "git.confirmSync": false,
  "git.autofetch": true,
  "editor.renderWhitespace": "all",
  "editor.fontLigatures": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
    "push:command"
  ],
  "editor.formatOnSave": true,
  "vim.smartRelativeLine": true,
  "workbench.sideBar.location": "right",
  "vim.enableNeovim": true,
  "vim.neovimUseConfigFile": true,
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "block",
  "vim.cursorStylePerMode.visualblock": "block",
  "vim.cursorStylePerMode.visualline": "block",
  "vim.useSystemClipboard": true,
  "vim.statusBarColors.visual": "colors.visual_grey",
  "vim.statusBarColors.visualblock": "colors.visual_grey",
  "vim.visualModeKeyBindings": [
    {
      "before": ["g", "b"],
      "commands": ["editor.action.blockComment"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "a", "w"],
      "commands": ["editor.action.quickFix"]
    }
  ],
  "vim.leader": " ",
  "vim.highlightedyank.enable": true,
  "editor.fontFamily": "JetBrains Mono, monospace, 'Courier New'",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.neovimConfigPath": "~/.config/nvim/init.lua",
  "vim.neovimPath": "/usr/bin/nvim",
  "workbench.colorTheme": "Atom One Dark",
  "vim.matchpairs": "(:),{:},[:],<:>",
  "editor.inlineSuggest.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "bloc.newBlocTemplate.type": "freezed",
  "dart.devToolsBrowser": "default",
  "dart.normalizeFileCasing": true,
  "github.copilot.enable": {
    "*": false,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "dart": true
  },
  "dartImport.fixOnSave": true,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.replace": "#D08770",
  "vim.statusBarColors.commandlineinprogress": "#007ACC",
  "vim.statusBarColors.searchinprogressmode": "#007ACC",
  "vim.statusBarColors.easymotionmode": "#007ACC",
  "vim.statusBarColors.easymotioninputmode": "#007ACC",
  "vim.statusBarColors.surroundinputmode": "#007ACC",
  "vsicons.dontShowNewVersionMessage": true,
  "vim.vimrc.enable": true,
  "vim.vimrc.path": "$HOME/.vim/vimrc",
  "settingsSync.ignoredExtensions": [
    "ms-python.python"
  ],
  "editor.cursorSurroundingLines": 8
}
